<Window x:Class="Lab1Ex1.WpfHello.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab1Ex1.WpfHello"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas>
        <Button x:Name="button" Content="Set Name" HorizontalAlignment="Left" Margin="150,150,0,0" VerticalAlignment="Top" Width="74" Click="Button_Click"/>
        <Button x:Name="button1" Content="Ret Name" HorizontalAlignment="Left" Margin="150,250,0,0" VerticalAlignment="Top" Width="74" Click="Button_Click_1"/>
        <Label x:Name="label" Content="" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="300,235,0,0" Width="456" Height="35"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Margin="300,150,0,0" Width="74" TextChanged="textBox_TextChanged"/>
        <Button Canvas.Left="10" Canvas.Top="10" Content="Click Me" Height="23" Name="button3" Width="75" Click="button3_Click"/>
        <TextBox Width="170" Canvas.Left="90" Canvas.Top="12" Name="myTextBox"/>
    </Canvas>
</Window>
